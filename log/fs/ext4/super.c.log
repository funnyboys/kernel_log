diff --git a/fs/ext4/super.c b/fs/ext4/super.c
index 844c95d95821..3aea58a7ea8f 100644
--- a/fs/ext4/super.c
+++ b/fs/ext4/super.c
@@ -1097,8 +1097,8 @@ static int bdev_try_to_free_page(struct super_block *sb, struct page *page,
