    mount /dev/vdb /mnt
    cd /mnt && mkdir dir && for i in {1..8}; do setfacl -dm "u:user_"$i":rx" dir; done
    mkdir dir/dir1 && mv dir/dir1 ./
    sh repro.sh && add some user
    
    [root@localhost ~]# cat repro.sh
