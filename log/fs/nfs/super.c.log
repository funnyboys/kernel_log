    https://tools.ietf.org/html/rfc7530:
    
    nfstime4
    
       struct nfstime4 {
               int64_t         seconds;
               uint32_t        nseconds;
       };
    
    https://tools.ietf.org/html/rfc1094
    
              struct timeval {
                  unsigned int seconds;
                  unsigned int useconds;
              };
    
    https://tools.ietf.org/html/rfc1813
    
    struct nfstime3 {
             uint32   seconds;
             uint32   nseconds;
          };
    
