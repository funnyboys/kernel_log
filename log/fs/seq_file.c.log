        int fd = open("/proc/self/maps", O_RDONLY);
    
        n = pread(fd, buf, sizeof(buf), 0);
        printf("%d\n", n);
    
        /* lseek(fd, 0, SEEK_CUR); */ /* Uncomment to work around */
    
        n = pread(fd, buf, sizeof(buf), 0);
        printf("%d\n", n);
    
