      $ make \
            TEST_CUSTOM_PROGS= \
            TEST_GEN_PROGS= \
            TEST_GEN_PROGS_EXTENDED= \
            TEST_PROGS_EXTENDED= \
            TEST_PROGS="test_xdp_veth.sh" \
            run_tests
