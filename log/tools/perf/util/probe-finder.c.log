--- a/tools/perf/util/probe-finder.c
+++ b/tools/perf/util/probe-finder.c
@@ -1087,6 +1087,7 @@ static int debuginfo__find_probes(struct debuginfo *dbg,
 struct local_vars_finder {
 	struct probe_finder *pf;
