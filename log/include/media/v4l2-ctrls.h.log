    req_validate() implementation:
    
            int res = -EINVAL;
    
            hdl = v4l2_ctrl_request_hdl_find(req, parent_hdl);
            if (hdl) {
                    if (v4l2_ctrl_request_hdl_ctrl_find(hdl, ctrl_id))
                            res = 0;
                    v4l2_ctrl_request_hdl_put(hdl);
            }
            return res;
    
