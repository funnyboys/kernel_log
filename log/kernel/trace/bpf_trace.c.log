       #5 [ffffc9002219f770] queued_spin_lock_slowpath at ffffffff8110be24
       #6 [ffffc9002219f770] _raw_spin_lock_irqsave at ffffffff81a43012
       #7 [ffffc9002219f780] try_to_wake_up at ffffffff810e7ecd
