    3: (7b) *(u64 *)(r10 -24) = r2
    4: (7b) *(u64 *)(r10 -32) = r2
    5: (7b) *(u64 *)(r10 -40) = r2
    6: (7b) *(u64 *)(r10 -48) = r2
    7: (bf) r2 = r10
    8: (07) r2 += -48
    9: (b7) r3 = 36
    10: (b7) r4 = 0
