+#include <core/os.h>
+#include <core/engctx.h>
+
+#include <engine/sw.h>
+
+struct nv10_sw_priv {
+	struct nouveau_sw base;
+};
+
+struct nv10_sw_chan {
+	struct nouveau_sw_chan base;
+};
+
+/*******************************************************************************
+ * software object classes
+ ******************************************************************************/
+
+static int
+nv10_sw_flip(struct nouveau_object *object, u32 mthd,
+		   void *args, u32 size)
+{
+	struct nv10_sw_chan *chan = (void *)nv_engctx(object->parent);
