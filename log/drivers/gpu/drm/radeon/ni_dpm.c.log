+int ni_power_control_set_level(struct radeon_device *rdev)
+{
+	ni_restrict_performance_levels_before_switch(rdev);
+	rv770_halt_smc(rdev);
+	ni_populate_smc_tdp_limits(rdev);
+	rv770_resume_smc(rdev);
+	rv770_set_sw_state(rdev);
+
+	return 0;
+}
