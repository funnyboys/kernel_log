            memcpy(pMICHDR+2, &(pMACHeader->addr2[0]), 6);
    __be32 tsc_47_16;
            pMICHDR+8 = HIBYTE(HIWORD(pTransmitKey->dwTSC47_16));
            pMICHDR+9 = LOBYTE(HIWORD(pTransmitKey->dwTSC47_16));
