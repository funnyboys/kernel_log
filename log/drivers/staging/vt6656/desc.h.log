    __be16 payload_len;
            pMICHDR+14 = HIBYTE(wPayloadLen);
            pMICHDR+15 = LOBYTE(wPayloadLen);
    __be16 hlen;
            pMICHDR+16 = 0; // HLEN[15:8]
            pMICHDR+17 = 28; // HLEN[7:0]
    __le16 frame_control;
            memcpy(pMICHDR+18, (u8 *)&wValue, 2); // MSKFRACTL
