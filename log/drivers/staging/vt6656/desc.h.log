    u8 addr2[6];
            memcpy(pMICHDR+26, &(pMACHeader->addr2[0]), 6);
    u8 addr3[6];
            memcpy(pMICHDR+32, &(pMACHeader->addr3[0]), 6);
    __le16 seq_ctrl;
            memcpy(pMICHDR+38, (u8 *)&wValue, 2); // MSKSEQCTL
    u8 addr4[6];
            memcpy(pMICHDR+40, &(pMACHeader->addr4[0]), 6);
    u16 packing; /* packing to 48 bytes */
    
