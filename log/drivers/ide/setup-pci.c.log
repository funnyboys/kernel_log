+void ide_setup_pci_noise (struct pci_dev *dev, ide_pci_device_t *d)
+{
+	printk(KERN_INFO "%s: IDE controller at PCI slot %s\n",
+			 d->name, pci_name(dev));
+}
+
+EXPORT_SYMBOL_GPL(ide_setup_pci_noise);
