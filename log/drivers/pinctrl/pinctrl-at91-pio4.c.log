+}
+#else
+#define atmel_gpio_irq_set_wake NULL
+#endif /* CONFIG_PM_SLEEP */
+
 static struct irq_chip atmel_gpio_irq_chip = {
 	.name		= "GPIO",
 	.irq_ack	= atmel_gpio_irq_ack,
 	.irq_mask	= atmel_gpio_irq_mask,
 	.irq_unmask	= atmel_gpio_irq_unmask,
