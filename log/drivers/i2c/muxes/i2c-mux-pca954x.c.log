    -        if (match) e = of_device_get_match_data(dev);
    -        else e = e1;
    +        e = of_device_get_match_data(dev);
    +        if (!e) e = e1;
    
    @@
    identifier r.i;
    @@
    - const struct of_device_id *i;
    ... when != i
    // </smpl>
    
    Signed-off-by: Julia Lawall <Julia.Lawall@lip6.fr>
    Signed-off-by: Peter Rosin <peda@axentia.se>

