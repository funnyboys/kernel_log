+int a2091_release(struct Scsi_Host *instance)
+{
+#ifdef MODULE
+	DMA(instance)->CNTR = 0;
+	release_mem_region(ZTWO_PADDR(instance->base), 256);
+	free_irq(IRQ_AMIGA_PORTS, instance);
+	wd33c93_release();
+#endif
+	return 1;
+}
