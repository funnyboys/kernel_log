      init_sniffer_rx_root_ns()
        cleanup_root_ns(steering->sniffer_rx_root_ns);
    mlx5_cleanup_fs()
      cleanup_root_ns(steering->sniffer_rx_root_ns);
      /* double free of sniffer_rx_root_ns */
    
