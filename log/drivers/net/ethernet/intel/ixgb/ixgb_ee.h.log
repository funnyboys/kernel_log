    0x09c3 ixgb_check_options [ixgb]:                       448
    0x0000649e ixgb_set_ringparam [ixgb]:                   192
    0x0000130d ixgb_xmit_frame [ixgb]:                      184
    0x000019e0 ixgb_xmit_frame [ixgb]:                      184
