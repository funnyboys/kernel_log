diff --git a/drivers/net/wireless/ath/ath9k/eeprom.c b/drivers/net/wireless/ath/ath9k/eeprom.c
index df41ed5fd7c4..9b1d960dc80f 100644
--- a/drivers/net/wireless/ath/ath9k/eeprom.c
