              .init : 0x(ptrval) - 0x(ptrval)   ( 144 KiB)
              .text : 0x(ptrval) - 0x(ptrval)   (2980 KiB)
              .data : 0x(ptrval) - 0x(ptrval)   (1095 KiB)
