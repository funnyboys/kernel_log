+void __iomem *pmu_base_addr;
+
 static struct map_desc exynos4_iodesc[] __initdata = {
 	{
 		.virtual	= (unsigned long)S3C_VA_SYS,
@@ -264,6 +267,39 @@ static void __init exynos_init_io(void)
 	exynos_map_io();
