                    echo "removing" $DEF; \
                    sed -i "s/${DEF}/${CH}/" arch/arm/mach-omap2/omap_hwmod_*.c; \
                    sed -i "/${DEF}/d" arch/arm/mach-omap2/dma.h; \
            done
    
