     02 0001 1   0    0    0   0   0    0     0    02
     03 0002 1   1    0    0   0   0    0     0    03
     04 0011 1   1    0    0   0   0    0     0    04
     05 0004 1   1    0    0   0   0    0     0    05
     06 0005 1   1    0    0   0   0    0     0    06
